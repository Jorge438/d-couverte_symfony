{% extends "base.html.twig" %}

{% block body %}
<div class="container mt-4">
<h1>{{ form_title }}</h1>
    {{ form_start(form) }}
    <div class="row">
        <div class="col-md-4"> {{form_row(form.name)}} </div>
        <div class="col-md-2"> {{form_row(form.age)}} </div>
    </div>
        {{form_rest(form)}}
    {{ form_end(form)}}
</div>
{% endblock %}